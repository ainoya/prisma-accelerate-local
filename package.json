{
  "name": "prisma-accelerate-local",
  "version": "0.0.9",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "test": "tsc && node dist/cjs/command postgresql://postgres:password@localhost:15432/postgres -p 8000",
    "lint:fix": "eslint --fix src && prettier -w src"
  },
  "bin": {
    "prisma-accelerate-local": "dist/cjs/command/index.js"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "dependencies": {
    "@colors/colors": "^1.6.0",
    "@prisma/client": "^5.6.0",
    "@prisma/fetch-engine": "^5.6.0",
    "fastify": "^4.24.3",
    "minimist": "^1.2.8",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.10.2",
    "@types/node-forge": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "repository": "https://github.com/node-libraries/prisma-accelerate-local",
  "author": "SoraKumo <info@croud.jp>"
}
